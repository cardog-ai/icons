{
  "name": "@cardog-icons/react-native",
  "version": "1.0.0",
  "description": "Car brand icons for React Native applications",
  "private": false,
  "license": "MIT",
  "author": "Cardog",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react --external react-native",
    "dev": "tsup src/index.ts --format esm,cjs --dts --external react --external react-native --watch",
    "clean": "rm -rf dist",
    "generate": "ts-node scripts/generate-icons.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-native": ">=0.60.0",
    "react-native-svg": ">=12.0.0"
  },
  "dependencies": {
    "@cardog-icons/core": "^1.0.0"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-svg": "^15.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "keywords": [
    "react-native",
    "icons",
    "svg",
    "car",
    "brand",
    "logos",
    "automotive"
  ],
  "publishConfig": {
    "access": "public"
  }
}